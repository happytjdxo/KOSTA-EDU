package step2;

class KostaEducation{
	int classNumber;
	String teacherName;
	String subject;

	// setKostaInfo() / getKostaInfo()
	public void setKostaInfo(int cn, String t, String s){
		classNumber = cn;
		teacherName = t;
		subject = s;
	}
	public String getKostaInfo(){
		return " classNumber : " + classNumber + " teacherName: "
		+ teacherName + " subject : " + subject;
	}
	public int getclassNumber(){
		return classNumber;
	}
	public String getteacherName(){
		return teacherName;
	}
	public String getsubject(){
		return subject;
	}
}