package step5.book.test;
class Card{
	int number;
	String kind;
	static int width = 100;
	static int height = 250;
}
public class CardTest {
	public static void main(String[] args) {
		
		System.out.println("Card.width = " + Card.width);
		System.out.println("Card.heigth = " + Card.height);
		
		Card c1 = new Card();
		c1.kind = "Heart";
		c1.number = 7;
		
		Card c2 = new Card();
		c2.kind = "Spade";
		c2.number = 4;
		
		System.out.println("c1 은 " + c1.kind + ", " + c1.number + "이고, Size는 ( " + "가로 " + c1.width + ", 세로 " +c1.height + "입니다.)");
		System.out.println("c2 는 " + c2.kind + ", " + c2.number + "이고, Size는 ( " + "가로 " + c2.width + ", 세로 " +c2.height + "입니다.)");
		
	}

}
