package market.proj.service;

import market.proj.vo.ProductVO;

public class MyDate {
	private int year;
	private int month;
	private int day;
	
	public MyDate(int y, int m, int d){
		this.year = y;
		this.month = m;
		this.day = d;
	}
	public String getMyDate(){
		return year + "년" + month + "월" + day + "일";
	}
}

	