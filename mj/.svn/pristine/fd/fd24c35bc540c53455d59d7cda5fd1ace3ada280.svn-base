package string.test;

public class StringTest11 {
	public static void main(String[] args) {
		String str = "Hello";
		System.out.println(str);
		//"Hello habaree"
		String str1 = str.concat("habaree");
		System.out.println(str1);
		//"Hello habaree Kosta"
		String str2 = str1.concat("Kosta");
		System.out.println(str2);
	}

}
