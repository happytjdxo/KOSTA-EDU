<%@ page language="java" contentType="text/html; charset=UTF-8"
   pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jstl/fmt" %>
<!DOCTYPE html>
<html>
<head>
<title>Anna Bootstarp Website Template | Home :: w3layouts</title>
<meta charset="UTF-8" http-equiv="X-UA-Compatible" content="IE=11" />
<script type="text/javascript"
   src="http://code.jquery.com/jquery-1.11.3.js"></script>
<script
   src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.16.1/TweenMax.min.js"></script>
<script type="text/javascript" src="js/subpagemenu2.js"></script>
<script type="text/javascript" charset="utf-8"
   src="js/jquery.leanModal.min.js"></script>
<link href="css/main/main.css" rel="stylesheet" type="text/css" />
<link href="css/common.css" rel="stylesheet" type="text/css" />
<link href="css/layout2.css" rel="stylesheet" type="text/css" />
<style type="text/css">
#menu {
   height: 100px;
   background-color: #a6ce39;
   margin: 0;
   padding: 0;
}

#subMenuBar {
   position: relative;
}

#loginmodal {
   border: 2px solid #808040;
   width: 260px;
   height: 240px
}

#custom-search-input {
   padding: 3px;
   border: solid 1px #E4E4E4;
   border-radius: 6px;
   background-color: #fff;
}

#custom-search-input input {
   border: 0;
   box-shadow: none;
}

#custom-search-input button {
   margin: 2px 0 0 0;
   background: none;
   box-shadow: none;
   border: 0;
   color: #666666;
   padding: 0 8px 0 10px;
   border-left: solid 1px #ccc;
}

#custom-search-input button:hover {
   border: 0;
   box-shadow: none;
   border-left: solid 1px #ccc;
}

#custom-search-input .glyphicon-search {
   font-size: 23px;
}
</style>
<!--bootstarp-css-->
<link href="css/bootstrap.css" rel="stylesheet" type="text/css"
   media="all" />
<!--/bootstarp-css -->
<!--css-->
<link rel="stylesheet" href="css/style.css" type="text/css" media="all" />
<!--/css-->
<!--fonts-->
<link
   href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,600,300,700'
   rel='stylesheet' type='text/css'>
<!--fonts-->
<!--js-->
<script src="js/jquery.min.js"></script>
<!--/js-->
<!-- animated-css -->
<link href="css/animate.css" rel="stylesheet" type="text/css"
   media="all">
<script src="js/wow.min.js"></script>
<script>
   new WOW().init();
</script>
<!-- animated-css -->
</head>


<!-- jhy  -->

<body>

   <div id="menu">
      <c:import url="header.jsp"></c:import>
   </div>
   <div id="loginmodal" style="display: none;">
      <h2 align="center">LOGIN</h2>
      <div class="p_c_text" align="center">오한자에 오신 것을 환영합니다.</div>
      <div class="login_line">
         <div class="box_in">
            <input type="text" name="id" id="id" size="23"> <input
               type="password" name="pw" id="pw" size="23">
         </div>
         <span class="btn_login"><a href="">LOGIN</a></span>
      </div>
      <div class="find_join">
         <a href="#" onclick="searchId()">아이디 / 비밀번호 찾기</a> | <a
            href="agreement.jsp">회원가입</a>
      </div>
   </div>

   <!-- jhy  -->
   <!--portfolio-->
   
   <div class="portfolio">
      <div class="portfolio-info">
         <div class="container" id="portfolio_form">
            <div class="col-sm-3 col-md-2 sidebar">
               <ul class="nav nav-sidebar">
                  <li class="active"><a href="#"> <img
                        src="img/main/subMenu_alcohol_house_btn/subpage2.jpg"> <span
                        class="sr-only">(current)</span></a></li>
                  <li><a href="#"><img id="subMenu1"
                        src="img/main/subMenu_alcohol_house_btn/bundang.png"></a></li>
                  <li><a href="#"><img id="subMenu2"
                        src="img/main/subMenu_alcohol_house_btn/jungwon.png"></a></li>
                  <li><a href="#"><img id="subMenu3"
                        src="img/main/subMenu_alcohol_house_btn/sujung.png"></a></li>
               </ul>
               <img alt="" id="subMenuBar"
                  src="img/main/subMenu_alcohol_tip_btn/bar.jpg" width="140px"
                  height="2px">
            </div>
            <div id="portfoliolist"   class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
            <c:forEach var="vo" items="${list}" varStatus="cnt">
            
                 
            
            
               <div class="portfolio icon mix_all" data-cat="icon"
                  style="display: inline-block; opacity: 1;">
                  <div class="portfolio-wrapper wow bounceIn capt"
                     data-wow-delay="0.4s">
                     <a href="board.do?command=getBoard&&no=${vo.no}" class="b-link-stripe b-animate-go"> <img
                        src="images/1.jpg" alt="" />
                        <div class="b-wrapper" align="center">
                           <h2 class="b-animate b-from-left b-delay03 ">${vo.title}</h2>
                           <p class="b-animate b-from-left b-delay03 ">${vo.content}</p>
                        </div>
                     </a>
                  </div>
            	</div>
            </c:forEach>
            </div> 

               <div class="row">

                  <div class="col-md-6" style="margin: 5%; margin-left: 13%;">

                     <div id="custom-search-input">
                <div class="input-group col-md-12">
                    <input type="text" class="form-control input-lg" placeholder="search..." />
                    <span class="input-group-btn">
                        <button class="btn btn-info btn-lg" type="button">
                            <i class="glyphicon glyphicon-search"></i>
                            <a href="views/write.jsp"><button id="writeBtn" type="submit" class="btn btn-default">write</button></a>
                        </button>
                    </span>
                </div>
            </div>
                  </div>
               </div>
            </div>

            <div class="clearfix"></div>
         </div>
      </div>
   </div>
</body>
</html>